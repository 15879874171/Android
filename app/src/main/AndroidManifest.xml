<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raspberry">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Raspberry"
        tools:replace="android:icon">
        <activity android:name=".ui.HBS.Hbs"></activity>
        <activity android:name=".ui.shimge.Shimge" />
        <activity android:name=".ui.equipment.Management" />
        <activity android:name=".ui.about_us.About_Us" />
        <activity android:name=".ui.feedback.Feedback" />
        <activity android:name=".ui.instructions.Instructions_three" />
        <activity android:name=".ui.instructions.Instructions_two" />
        <activity android:name=".ui.instructions.Instructions_one" />
        <activity android:name=".ui.add_three.AddEquiment3" />
        <activity android:name=".ui.add_two.AddEquiment2" />
        <activity android:name=".ui.add_one.AddEquipment" />
        <activity android:name=".ui.record.Record" />
        <activity android:name=".ui.role.Role" />
        <activity android:name=".ui.controlpanel.ControlPanel" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>